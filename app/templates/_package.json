{
  "name": "<%= _.slugify(projectName) %>",
  "version": "0.0.1",
  "dependencies": {<% if (!singlePageApplication || jsFramework === 'marionette') { %>
    "jquery": "~2.1.3",<% } %>
    "normalize.css": "~3.0.1"<% if (jsFramework === 'angular') { %>,
    "angular": "~1.3.15",
    "angular-route": "~1.3.15",
    "json3": "~3.3.1"<% } %><% if (jsFramework === 'marionette') { %>,
    "backbone": "~1.1.2",
    "backbone.marionette": "~2.4.1",
    "underscore": "~1.8.3"<% } %><% if (jsOption === 'requirejs') { %>,
    "requirejs": "~2.1.11"<% } %><% if (jsFramework === 'react') { %>,
    "react-router": "~0.13.3",
    "superagent": "~1.2.0",
    "reflux": "~0.2.7",
    "node-jsx": "~0.12.4",
    "react": "~0.13.0",
    "babelify": "~6.0.2"<% } %><% if (useServer) { %><% if (jsFramework === 'react') { %>,<% } %>
    "lodash": "~3.0.0",
    "colors": "~0.6.2",
    "async": "~0.7.0",
    "body-parser": "~1.0.2",
    "cookie-parser": "~1.3.3",
    "method-override": "~2.3.0",
    "compression": "~1.0.1",
    "serve-favicon": "2.0.1",
    "errorhandler": "~1.0.0",
    "express": "~4.12.0",
    "express-session": "~1.6.0",
    "lusca": "~1.1.0",<% if (dbOption === 'mongodb') { %>
    "mongoose": "~4.0.0",<% } %>
    "morgan": "~1.0.0",
    "ejs": "~0.8.4"<% if (dbOption === 'sql') { %>,<% if (dbType === 'mysql') { %>
    "mysql": "2.3.2",<% } else if (dbType === 'postgres') { %>
    "pg": "~4.1.1",<% } %>
    "sequelize": "~2.1.0"<% } %><% if (htmlOption === 'swig') { %>,
    "swig": "~1.4.2"<% } %><% if (htmlOption === 'jade') { %>,
    "jade": "~1.9.2"<% } %><% } %>
  },
  "devDependencies": {<% if (jsFramework === 'angular') { %>
    "browserify-ngannotate": "0.7.2",
    "angular-mocks": "~1.3.15",<% } else if (jsFramework === 'marionette') { %>
    "jstify": "0.9.0",<% } %>
    "rimraf": "~2.2.0",
    "grunt": "~0.4.5",<% if (useServer) { %>
    "grunt-open": "~0.2.0",<% if (useServerTesting) { %>
    "grunt-mocha-test": "~0.12.7",
    "supertest": "~0.15.0",<% } %><% } else { %>
    "grunt-browser-sync": "~2.1.2",<% if (htmlOption === 'swig') { %>
    "swig": "~1.4.2",<% } %><% if (htmlOption === 'jade') { %>
    "jade": "~1.9.2",<% } %><% } %>
    "grunt-contrib-watch": "~0.6.1",
    "grunt-contrib-clean": "0.5.0",<% if (htmlOption === 'jade') { %>
    "grunt-contrib-jade": "~0.14.1",<% } %><% if (htmlOption === 'swig') { %>
    "grunt-swig-templates": "~0.1.2",<% } %>
    "grunt-postcss": "~0.4.0",
    "autoprefixer-core": "~5.2.0",
    "grunt-contrib-htmlmin": "~0.3.0",<% if (jsOption === 'browserify') { %>
    "minifyify": "~7.0.0",
    "envify": "~3.4.0",
    "grunt-browserify": "~3.8.0",<% } %>
    "grunt-concurrent": "~1.0.0",<% if (jsFramework === 'react') { %>
    "grunt-react": "~0.12.0",<% } %><% if (useE2e) { %>
    "protractor": "~2.0.0",
    "grunt-protractor-runner": "~2.0.0",
    "chromedriver": "~2.13.0 ",<% if (testFramework === 'mocha') { %>
    "mocha": "~2.1.0",
    "chai": "~1.10.0",
    "chai-as-promised": "4.1.1",<% } %><% } %>
    "grunt-contrib-imagemin": "~0.9.4",
    "imagemin-svgo": "~4.0.0",
    "imagemin-pngquant": "~4.0.0",
    "grunt-eslint": "~7.0.1",
    "grunt-contrib-copy": "~0.7.0",<% if (cssOption === 'sass') { %>
    "grunt-sass": "~1.0.0",<% } %><% if (cssOption === 'less') { %>
    "grunt-contrib-less": "~0.12.0",<% } %><% if (cssOption === 'stylus') { %>
    "grunt-contrib-stylus": "~0.20.0",<% } %>
    "grunt-open": "~0.2.3",<% if (jsFramework === 'angular') { %>
    "grunt-angular-templates": "~0.5.7",<% } %>
    "grunt-env": "~0.4.1",
    "grunt-newer": "~1.1.0",
    "include-all": "~0.1.6",<% if (jsOption === 'browserify') { %>
    "browserify" : "~10.0.0",<% } else if (jsOption === 'requirejs') { %>
    "karma-requirejs": "~0.2.1",<% } %><% if (useTesting) { %>
    "grunt-karma": "~0.9.0",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma-chrome-launcher": "~0.1.2",<% if (testFramework === 'jasmine') { %>
    "karma-jasmine": "~0.2.0",<% } else if (testFramework === 'mocha') { %>
    "karma-chai": "~0.1.0",
    "karma-mocha": "~0.1.3",<% } %>
    "karma": "~0.12.21",<% } %><% if (!useServer && useDashboard) { %>
    "lodash": "~3.0.0",<% } %>
    "time-grunt": "0.2.10",<% if (useDashboard) { %>
    "grunt-dashboard": "~1.0.2",<% } %>
    "jit-grunt": "~0.9.0"<% if (useServer) { %>,
    "connect-livereload": "~0.5.2",
    "grunt-express-server": "~0.4.5"<% } %>
  },
  "scripts": {
    "clean": "rimraf node_modules tmp build"<% if (useServer) { %>,
    "start": "NODE_ENV=production node server/server.js"<% } %><% if (useE2e) { %>,
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update"<% } %>
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
